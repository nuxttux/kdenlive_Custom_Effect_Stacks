<effectgroup description="Simple zoom-out transition with lens destortion. Cut 8 frames from clip to apple effect." id="Zoom Out Transition 8f" parentIn="0">
 <description>Simple zoom-out transition with lens destortion. Cut 8 frames from clip to apple effect.</description>
 <effect id="box_blur">
  <property name="kdenlive:collapsed">1</property>
  <property name="vradius">0h=5;7=0</property>
  <property name="preserve_alpha">0</property>
  <property name="hradius">0h=5;7=0</property>
 </effect>
 <effect id="pan_zoom">
  <property name="kdenlive:collapsed">1</property>
  <property name="transition.repeat_off">1</property>
  <property name="producer.resource">0x00000000</property>
  <property name="use_normalised">1</property>
  <property name="transition.distort">1</property>
  <property name="transition.mirror_off">1</property>
  <property name="transition.rect">0h=-1920 -1080 5760 3240;7=0 0 1920 1080</property>
 </effect>
  <property name="kdenlive:collapsed">1</property>
 <effect id="frei0r.lenscorrection">
  <property name="brightness">0h=0;7=0</property>
  <property name="correctionnearedges">0h=0.194;7=0.5</property>
  <property name="correctionnearcenter">0h=0.435;7=0.5</property>
  <property name="ycenter">0h=0.5;7=0.5</property>
  <property name="xcenter">0h=0.5;7=0.5</property>
 </effect>
</effectgroup>
